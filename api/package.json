        "@hapi/inert": "^6.0.1",
        "@hapi/joi": "^17.1.1",
        "@nodepit/migrate-state-store-mongodb": "^2.0.0",
        "archiver": "^4.0.1",
        "aws-sdk": "^2.693.0",
        "bcryptjs": "^2.4.3",
        "bson": "^4.0.4",
        "crypto-random-string": "^3.2.0",
        "dateformat": "^3.0.3",
        "deep-equal": "^2.0.3",
        "diff": "^4.0.2",
        "dotenv-safe": "^6.0.0",
        "fast-diff": "^1.2.0",
        "geolib": "^3.3.1",
        "handlebars": "^4.7.6",
        "hapi-auth-jwt2": "^10.1.0",
        "hapi-require-https": "^5.0.0",
        "js-beautify": "^1.11.0",
        "marked": "^1.1.0",
        "migrate": "^1.7.0",
        "moment": "^2.26.0",
        "mongoose": "^5.9.18",
        "node-stream-zip": "^1.11.2",
